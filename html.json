			"\t\t<tr>",
			"\t\t\t<td><label for=\"address\">Address:</label></td>",
			"\t\t\t<td><input data-dojo-type=\"dijit/form/TextBox\" name=\"address\" id=\"address\"></td>",
			"\t\t</tr>",
			"\t</table>",
